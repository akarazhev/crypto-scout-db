# Example env file for TimescaleDB
# Copy to ./secrets/timescaledb.env and replace the value
# POSTGRES_USER and POSTGRES_PASSWORD must be 20-255 characters. Use only ASCII letters/numbers for simplicity.

# TimescaleDB connection
POSTGRES_DB=crypto_scout
POSTGRES_USER=crypto_scout_db
POSTGRES_PASSWORD=CHANGEME_ExamplePasswordValue1234567890
TIMESCALEDB_TELEMETRY=off

# TimescaleDB tuning parameters
TIMESCALEDB_TUNE_MAX_CONNECTIONS=100
TIMESCALEDB_TUNE_MAX_BACKGROUND_WORKERS=8
TIMESCALEDB_TUNE_MAX_PARALLEL_WORKERS=4
TIMESCALEDB_TUNE_MEMORY=512MB